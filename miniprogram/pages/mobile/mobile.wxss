/* pages/mobile/mobile.wxss */

.page-section {
    margin-top: 40rpx;
    height: 100rpx;
    /* box-shadow: h-shadow v-shadow blur spread color inset; */
    box-shadow: 0 6rpx 10rpx 2rpx #cccccc;
    -webkit-box-shadow: 0 3px 5px 1px #cccccc;
    display: flex;
    align-items: center;
    justify-content: center;
}
.page-section navigator {
    width: 646rpx;
    margin: 0 auto;
    height: fit-content;
    display: flex;
    justify-content: space-between;
}
.page-section navigator view {
    float: left;
}

.in-coming {
    width: 100%;
    margin: 0 auto;
    height: 1200rpx;
    line-height: 330rpx;
    text-align: center;
  }
  .in-coming image {
    height: 100%;
    width: 100%;
  }