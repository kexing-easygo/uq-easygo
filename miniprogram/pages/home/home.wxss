/* pages/home/home.wxss */
swiper {
    width: 600rpx;
    /* height: calc(632rpx * 316 / 500); */
    height: 274rpx;
    margin: 0 auto;
    margin-top: 36rpx;
    overflow: hidden;
    border-radius: 38rpx;
}
swiper image {
    width: 100%;
    height: 100%;
}
.nav-icons, .nav-text {
    height: 88rpx;
    width: 530rpx;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
}
.nav-icons {
    margin-top: 72rpx;
}
.nav-text {
    margin-top: 14rpx;
    height: 48rpx;
    margin-bottom: 60rpx;
}
.nav-icons navigator, .nav-text navigator {
    height: 100%;
    width: 88rpx;
    float: left;
    font-size: 22rpx;
    text-align: center;
}
.nav-icons image {
    height: 100%;
    width: 88rpx;
}
.in-coming {
    width: 642rpx;
    margin: 0 auto;
    height: 342rpx;
    line-height: 330rpx;
    text-align: center;
    border: 1px solid #707070;
}