/* pages/email/email.wxss */

.page-section {
    margin-top: 40rpx;
    height: 100rpx;
    box-shadow: 0 6rpx 10rpx 2rpx #cccccc;
    -webkit-box-shadow: 0 3px 5px 1px #cccccc;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #ffffff;
    font-size: 34rpx;
    color: (0, 0, 0, 0.9);
}
.page-section navigator {
    width: 646rpx;
    margin: 0 auto;
    height: fit-content;
    display: flex;
    justify-content: space-between;
}
.page-section navigator view {
    float: left;
}