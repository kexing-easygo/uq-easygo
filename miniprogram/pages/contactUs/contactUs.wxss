/* miniprogram/pages/contactUs/contactUs.wxss */
.container {
  height: 100%;
  width:100%;
  display: flex;
  justify-content: center;
  flex-flow: column nowrap;
  align-items: center;
}

.header {
  width: 100%; 
  text-align: center;
  font-weight: 300;
  margin-top:150rpx;
  margin-bottom: 40rpx;
}

.header image {
  width: 140rpx;
  height: 140rpx;
  margin-bottom: 40rpx;
}

.feedback {
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  box-shadow: 0 10rpx 10rpx 2rpx #cccccc;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

