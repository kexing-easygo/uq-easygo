<view class="setting-input-area">
	<input
	 class="user-input"
	 id="input1"
	 bindinput="bindKeyboardInput"
	 placeholder="请输入邮箱"
	 class="setting-input"
	/>
</view>
<view>
	<button
	 bindtap="sendVerificationCode"
	 disabled="{{disabledButton}}"
	 class="setting-confirm-button"
	>获取验证码
	</button>
</view>

