<!--pages/getMobile/getMobile.wxml-->
<view class="setting-input-area">
	<input
	 class="user-input"
	 id="input1"
	 bindinput="bindKeyboardInput"
	 placeholder="请输入手机号码"
	 class="setting-input"
	/>
</view>
<view style="margin-top: 52rpx;">
	<button
	 bindtap="sendVerificationCode"
	 disabled="{{disabledButton}}"
	 class="setting-confirm-button"
	>获取验证码
	</button>
</view>

